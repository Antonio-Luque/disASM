E1E4 01 05 00    2 149 ld      bc, 5                       ; BC = length of string
E1E7 CD 3C 20    2 150 call    PR_STRING                   ; Print string
E1EA FD CB 01 AE 2 151 res     5, (iy+1)                   ; Set key pressed flag = off
E1EE             2 152 Wait_More:
